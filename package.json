{
  "name": "mylists",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle --external:fsevents",
    "dev": "run-p dev:*",
    "dev:server": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:remix": "remix dev --manual -c \"node --require ./mocks ./build/server.js\"",
    "format": "prettier --write .",
    "generate": "prisma generate",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint --cache --cache-location ./node_modules/.cache/eslint --fix .",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:reset": "prisma migrate reset --force --skip-generate",
    "seed": "prisma db seed",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "test": "vitest",
    "pretest:e2e:run": "npm run build",
    "typecheck": "tsc",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/postgres-data"
  ],
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@fontsource/inter": "^5.0.16",
    "@hello-pangea/dnd": "^16.5.0",
    "@isaacs/express-prometheus-middleware": "^1.2.1",
    "@mui/icons-material": "^5.15.4",
    "@mui/material": "^5.15.4",
    "@mui/x-date-pickers": "^6.18.7",
    "@paralleldrive/cuid2": "^2.2.2",
    "@prisma/client": "^5.8.0",
    "@remix-run/css-bundle": "^2.4.1",
    "@remix-run/express": "^2.4.1",
    "@remix-run/node": "^2.1.0",
    "@remix-run/react": "^2.4.1",
    "@remix-validated-form/with-zod": "^2.0.7",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^18.19.6",
    "bcryptjs": "^2.4.3",
    "chokidar": "^3.5.3",
    "classnames": "^2.5.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "isbot": "^3.8.0",
    "morgan": "^1.10.0",
    "papaparse": "^5.4.1",
    "prisma": "^5.8.0",
    "prom-client": "^14.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-typedjson": "^0.3.1",
    "remix-validated-form": "^5.1.5",
    "sonner": "^1.3.1",
    "source-map-support": "^0.5.21",
    "tiny-invariant": "^1.3.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@remix-run/dev": "^2.4.1",
    "@remix-run/eslint-config": "^2.4.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@types/compression": "^1.7.5",
    "@types/eslint": "^8.56.2",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/papaparse": "^5.3.14",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/source-map-support": "^0.5.10",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "cookie": "^0.5.0",
    "dotenv": "^16.3.1",
    "esbuild": "^0.19.11",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-css-import-order": "^1.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-sort-exports": "^0.8.0",
    "eslint-plugin-typescript-sort-keys": "^3.1.0",
    "happy-dom": "^10.11.2",
    "msw": "^1.3.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "start-server-and-test": "^2.0.3",
    "vite": "^4.5.1",
    "vite-tsconfig-paths": "^3.6.0",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register prisma/seed.ts"
  }
}
